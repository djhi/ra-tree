{
    "name": "ra-tree-core",
    "version": "1.1.2",
    "description": "A treeview component without ui to use with react-admin",
    "main": "lib/index.js",
    "files": [
        "LICENSE",
        "*.md",
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/djhi/ra-tree.git"
    },
    "keywords": [
        "reactjs",
        "react",
        "react-admin",
        "admin-on-rest",
        "rest",
        "treeview"
    ],
    "author": "Gildas Garcia",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/djhi/ra-tree/issues"
    },
    "homepage": "https://github.com/djhi/ra-tree#readme",
    "scripts": {
        "build": "rimraf ./lib && cross-env NODE_ENV=production babel --quiet ./src -d ./lib --ignore spec.js,test.js",
        "prepublish": "yarn run build",
        "watch": "rimraf ./lib && cross-env NODE_ENV=production babel --watch ./src -d ./lib --ignore spec.js,test.js"
    },
    "peerDependencies": {
        "react": "~16.0.0 || ~16.1.0 || ~16.2.0 || ~16.3.0 || ~16.4.0",
        "react-dom": "~16.0.0 || ~16.1.0 || ~16.2.0 || ~16.3.0 || ~16.4.0"
    },
    "dependencies": {
        "performant-array-to-tree": "^1.1.2",
        "prop-types": "^15.6.1"
    },
    "devDependencies": {
        "babel-cli": "~6.26.0",
        "cross-env": "^5.2.0",
        "rimraf": "^2.6.2"
    }
}
